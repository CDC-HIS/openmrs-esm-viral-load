{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "fhir2": ">=1.2",
    "webservices.rest": "^2.24.0"
  },
  "extensions": [
    {
      "name": "Red box",
      "component": "redBox",
      "slot": "Boxes"
    },
    {
      "name": "Blue box",
      "component": "blueBox",
      "slot": "Boxes"
    },
        {
      "name": "Brand box",
      "component": "blueBox",
      "slot": "Boxes"
    },
    {
      "name": "ettors-current-widget",
      "component": "ettorsCurrent",
      "slot": "ettors-dashboard-slot",
      "online": true,
      "offline": true,
      "meta": {
        "fullWidth": true
      },
      "order": 1
    },
    {
      "name": "ettors-summary-widget",
      "component": "ettorsSummary",
      "slot": "ettors-dashboard-slot",
      "online": true,
      "offline": true,
      "meta": {
        "fullWidth": true
      },
      "order": 2
    },    
    {
      "name": "ettors-summary-dashboard",
      "component": "ettorsDashboardLink",
      "slot": "patient-chart-dashboard-slot",
      "online": true,
      "offline": true,
      "order": 6,
      "meta": {
        "columns": 1,
        "slot": "ettors-dashboard-slot",
        "path": "ettors"
      }
    },
    {
      "name": "encounter-delete-confirmation-dialog",
      "component": "encounterDeleteConfirmationDialog"
    }
  ],
  "pages": [
    {
      "component": "root",
      "route": "root"
    }
  ],
  "workspaces": [
    {
      "name": "ettors-workspace",
      "title": "VIRAL LOAD",
      "component": "ettorsWorkspace",
      "type": "form",
      "canMaximize": true,
      "canHide": false,
      "width": "wider"
    },
    {
      "name": "viral-load-result-workspace",
      "title": "VIRAL LOAD RESULT",
      "component": "vlResultWorkspace",
      "type": "form",
      "canMaximize": true,
      "canHide": false
    }
  ]
}
